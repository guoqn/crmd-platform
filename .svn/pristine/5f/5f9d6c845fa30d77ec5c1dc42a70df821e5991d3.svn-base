package com.ffcs.crmd.platform.data.sqlext.sqldata.dao.entity;

import javax.persistence.Column;
import javax.persistence.Id;
import javax.persistence.Table;

import com.ffcs.crmd.platform.data.entity.impl.CrmBaseEntityImpl;

@Table(name = "COM_DATA_VIEWER")
public class ComDataViewer extends CrmBaseEntityImpl<Long> {

	private static final long serialVersionUID = -2811347047325737314L;

	/**
	 * 标识
	 */
	@Id
	@Column(name = "ID")
	private Long id;

	/**
	 * 类型
	 */
	@Column(name = "TYPE")
	private String type;

	/**
	 * 名称
	 */
	@Column(name = "NAME")
	private String name;

	/**
	 * 脚本
	 */
	@Column(name = "DATA_SQL")
	private String dataSql;

	/**
	 * 关联对象ID
	 */
	@Column(name = "OBJ_ID")
	private Long objId;

	/**
	 * 
	 */
	@Column(name = "PAGE_SIZE")
	private Long pageSize;

	/**
	 * 排序
	 */
	@Column(name = "SEQ")
	private Long seq;

	/**
	 * 备注
	 */
	@Column(name = "REMARK")
	private String remark;

	/**
	 * 数据操作说明
	 */
	@Column(name = "ACT_DESC")
	private String actDesc;

	/**
	 * 
	 */
	@Column(name = "RELA_TABLE")
	private String relaTable;

	/**
	 * 
	 */
	@Column(name = "CHECK_FLAG")
	private Long checkFlag;

	/**
	 * 数据说明
	 */
	@Column(name = "DATA_DESC")
	private String dataDesc;

	/**
	 * 
	 */
	@Column(name = "ISMULTI")
	private Long ismulti;

	public void setId(Long id) {
		this.id = id;
	}

	public Long getId() {
		return this.id;
	}

	public void setObjId(Long objId) {
		this.objId = objId;
	}

	public Long getObjId() {
		return this.objId;
	}

	public void setPageSize(Long pageSize) {
		this.pageSize = pageSize;
	}

	public Long getPageSize() {
		return this.pageSize;
	}

	public void setDataSql(String dataSql) {
		this.dataSql = dataSql;
	}

	public String getDataSql() {
		return this.dataSql;
	}

	public void setSeq(Long seq) {
		this.seq = seq;
	}

	public Long getSeq() {
		return this.seq;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getRemark() {
		return this.remark;
	}

	public void setActDesc(String actDesc) {
		this.actDesc = actDesc;
	}

	public String getActDesc() {
		return this.actDesc;
	}

	public void setRelaTable(String relaTable) {
		this.relaTable = relaTable;
	}

	public String getRelaTable() {
		return this.relaTable;
	}

	public void setCheckFlag(Long checkFlag) {
		this.checkFlag = checkFlag;
	}

	public Long getCheckFlag() {
		return this.checkFlag;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getType() {
		return this.type;
	}

	public void setDataDesc(String dataDesc) {
		this.dataDesc = dataDesc;
	}

	public String getDataDesc() {
		return this.dataDesc;
	}

	public void setIsmulti(Long ismulti) {
		this.ismulti = ismulti;
	}

	public Long getIsmulti() {
		return this.ismulti;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
