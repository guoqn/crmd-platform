public class PageSqlEntity {

    private String pageSql;

    private Object[] pageParams;

    public PageSqlEntity(String pageSql) {
        this.pageSql = pageSql;
        this.pageParams = new Object[0];
    }

    public PageSqlEntity(String pageSql, Object... pageParams) {
        this.pageSql = pageSql;
        this.pageParams = pageParams;
    }

    public String getPageSql() {
        return pageSql;
    }

    public Object[] getPageParams() {
        return pageParams;
    }
}
