package com.ffcs.crmd.platform.meta.api.dto;

import com.ffcs.crmd.platform.core.ddd.api.dto.impl.DomBaseDTO;

public class DynamicWindowDTO extends DomBaseDTO {

	/**
	 * 事件序号
	 */
	private Long eventId;

	/**
	 * 窗体类型：模板、组合组件
	 */
	private String windowType;

	/**
	 * 分片键
	 */
	private Long shardingId;

	/**
	 * 窗体编码
	 */
	private String windowCode;

	/**
	 * 模板序号
	 */
	private Long templateId;

	/**
	 * 租户标识
	 */
	private Long tenantId;

	/**
	 * 窗体序号
	 */
	private Long windowId;

	/**
	 * 窗体名称
	 */
	private String windowName;

	/**
	 * 窗体描述
	 */
	private String windowDesc;

	// 组件-DTO
	private DynamicComponentDTO componentDto;

	public void setEventId(Long eventId) {
		this.eventId = eventId;
	}

	public Long getEventId() {
		return this.eventId;
	}

	public void setWindowType(String windowType) {
		this.windowType = windowType;
	}

	public String getWindowType() {
		return this.windowType;
	}

	public void setShardingId(Long shardingId) {
		this.shardingId = shardingId;
	}

	public Long getShardingId() {
		return this.shardingId;
	}

	public void setWindowCode(String windowCode) {
		this.windowCode = windowCode;
	}

	public String getWindowCode() {
		return this.windowCode;
	}

	public void setTemplateId(Long templateId) {
		this.templateId = templateId;
	}

	public Long getTemplateId() {
		return this.templateId;
	}

	public void setTenantId(Long tenantId) {
		this.tenantId = tenantId;
	}

	public Long getTenantId() {
		return this.tenantId;
	}

	public void setWindowId(Long windowId) {
		this.windowId = windowId;
	}

	public Long getWindowId() {
		return this.windowId;
	}

	public void setWindowName(String windowName) {
		this.windowName = windowName;
	}

	public String getWindowName() {
		return this.windowName;
	}

	public DynamicComponentDTO getComponentDto() {
		return componentDto;
	}

	public void setComponentDto(DynamicComponentDTO componentDto) {
		this.componentDto = componentDto;
	}

	public String getWindowDesc() {
		return windowDesc;
	}

	public void setWindowDesc(String windowDesc) {
		this.windowDesc = windowDesc;
	}
}
