package com.ffcs.crmd.platform.mq;

/**
 * 
 * 功能说明:Mq自定义异常
 *
 * @author ZHONGFUHUA
 * 
 * @Date 2015年10月30日 下午3:03:25
 *
 *
 * 版本号  |   作者   |  修改时间   |   修改内容
 *
 */
public class MqException extends Exception {
    private static final long serialVersionUID = 4004092473869661050L;
    
    /**
     * 构造
     * 
     * @param msg
     *            系统内部异常描述信息
     */
    public MqException(String msg) {
        super(msg);
    }
    
    /**
     * 构造
     * 
     * @param cause
     *            异常对象
     */
    public MqException(Throwable cause) {
        super(cause);
    }
    
    /**
     * 构造
     * 
     * @param msg
     *            系统内部异常描述信息
     * @param cause
     *            异常对象
     */
    public MqException(String msg, Throwable cause) {
        super(msg == null ? ""
            : msg, cause);
    }
}
